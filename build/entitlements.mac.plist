<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Allow the app to be run without notarization during development -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <false/>

    <!-- Allow JIT compilation for JavaScript -->
    <key>com.apple.security.cs.allow-jit</key>
    <true/>

    <!-- Disable library validation (required for Electron) -->
    <key>com.apple.security.cs.disable-library-validation</key>
    <true/>

    <!-- Allow the app to load plugins signed with different team IDs -->
    <key>com.apple.security.cs.allow-dyld-environment-variables</key>
    <true/>
</dict>
</plist>
